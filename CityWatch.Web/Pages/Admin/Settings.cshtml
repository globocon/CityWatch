        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title"><i class="fa fa-calendar mr-2"></i>Timesheet</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <input type="hidden" id="TimesheetGuard_Id" />
                        <h>Instant Timesheet</h>
                        <div class="form-group">
                            <label for="frequency"></label>
                            <select class="form-control" id="frequency" name="frequency">
                                <option value="">Select</option>
                                <option value="Today">Today</option>
                                <option value="ThisWeek">This Week</option>
                                <option value="Last2weeks">Last 2 weeks(fortnight)</option>
                                <option value="Last4weeks">Last 4 weeks(month)</option>
                                <option value="Month">Month Only</option>
                            </select>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <button type="button" id="btnDownloadTimesheetFrequency" class="btn btn-primary"><i class="fa fa-save mr-2"></i>Download</button>
                            </div>
                        </div><br />
                        <h>Custom Timesheets</h><br />
                        <div class="form-group">
                        </div>
                        <div class="form-group">
                            <label for="startDate">Start Date</label>
                            <input type="date" class="form-control" id="startDate" name="startDate">
                        </div>
                        <div class="form-group">
                            <label for="endDate">End Date</label>
                            <input type="date" class="form-control" id="endDate" name="endDate">
                        </div>


                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <button type="button" id="btnDownloadTimesheet" class="btn btn-primary"><i class="fa fa-save mr-2"></i>Download</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal" tabindex="-1" role="dialog" id="schedule-modal" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header p-2">
                <h5 class="modal-title"><i class="fa fa-link" aria-hidden="true"></i>Client(Site) SOP</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body p-2">
                <div id="loader"></div>
                <form id="frm_kpi_schedule" enctype="multipart/form-data" method="post">
                    <input type="hidden" id="scheduleId" name="id" value="0">
                    <div class="row">
                        <div class="col-md-12">

                            <div class="form-group mb-1">
                                <label for="clientTypeNamesiteSOP">Client Type</label>
                                <select class="form-control" id="clientTypeNamesiteSOP" name="clientTypeNamesiteSOP">

                                    <option value="">All</option>
                                    @foreach (var item in Model.ViewDataService.GetUserClientTypesHavingAccess(null))
                                    {
                                        <option value="@item.Name">@item.Name</option>
                                    }

                                </select>
                            </div>
                            <div class="form-group mb-1">
                                <label for="clientSitessiteSOP">Client Site</label>
                                <select class="form-control" id="clientSitessiteSOP" name="clientSitessiteSOP">
                                    <option>Select</option>
                                </select>
                            </div>
                            <div class="form-group mb-1">
                                <label for="clientSitessiteSOP">SOP</label>
                                <select class="form-control" id="SOP" name="SOP">
                                    <option></option>
                                    <option value="LB/KV">LB/KV</option>
                                </select>
                            </div>
                            <div class="form-group mb-1">

                                <label for="File">File</label>
                                <input type="file" name="add_staff_document_file_SlientSite_sop" id="add_staff_document_file_SlientSite_sop" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <ul id="sch-modal-validation" class="text-danger hide"></ul>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer p-2">
                <button type="button" id="btnSaveSiteSOP" class="btn btn-primary"><i class="fa fa-save mr-2"></i>Save Site SOP</button>
            </div>
        </div>
    </div>
</div>
