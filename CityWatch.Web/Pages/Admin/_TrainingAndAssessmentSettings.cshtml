<div class="modal fade" id="selectCoursesForGuardByAdmin" data-backdrop="static" data-keyboard="false" tabindex="-1" aria-labelledby="selectRadioCheckStatusActiveLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Course Library</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <input type="hidden" id="trainingguardId" value="" />
                    <ul id="courseList" class="list-group">
                        @foreach(var itemnew in configDataProvider.GetHRGroupsDropDown())
                        {
                            
                        if (@configDataProvider.GetTrainingCoursesStatusWithOutcome(Convert.ToInt32(@itemnew.Value)) != null)
                        {
                                <li class="list-group-item list-group-item-success" id="attach_@itemnew.Value" data-index="@itemnew.Value" style="border-left: 0;border-right: 0;font-size:12px;padding:3px">



                                </li>
                                foreach (var item in @configDataProvider.GetTrainingCoursesStatusWithOutcome(Convert.ToInt32(@itemnew.Value)))
                            {
                                
                                if (!string.IsNullOrWhiteSpace(item.FileName))
                                {
                                    
                                        <li class="list-group-item" id="attach_@item.Id" data-index="@item.Id" style="border-left: 0;border-right: 0;font-size:12px;padding:3px">
                                        @item.FileName
                                            <i class="fa fa-check ml-2 text-success btn-select-course-status" title="Select" style="cursor: pointer;float:right"></i>

                                        </li>

                                    

                                }
                                else
                                {
                                    <li class="list-group-item list-group-item-success" id="attach_@item.Id" data-index="@item.Id" style="border-left: 0;border-right: 0;font-size:12px;padding:3px">



